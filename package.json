{
  "name": "gibbs-scraper",
  "version": "1.0.0",
  "description": "boilerplate for express with babel7 designed to run on Google App Engine Standard enviornment",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon src/app.js --ignore links --ignore external --ignore data  --exec babel-node",
    "prestart": "npm run build",
    "start": "node dist/app.js",
    "build": "babel src --out-dir dist --copy-files",
    "lint": "eslint src"
  },
  "author": "Brendan Kennedy",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.3.0",
    "nodemon": "^1.18.5"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "backoff": "^2.5.0",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "cheerio-tableparser": "^1.0.1",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "express": "^4.16.4",
    "firebase-admin": "^6.1.0",
    "fs-extra": "^7.0.0",
    "helmet": "^3.14.0",
    "mongodb": "^3.1.8",
    "morgan": "^1.9.1",
    "response-time": "^2.3.2",
    "retry": "^0.12.0",
    "uuid": "^3.3.2"
  }
}
